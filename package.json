{
  "name": "@venizia/lib",
  "version": "0.0.0",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "description": "JS/TS Server Infrastructure with Hono Framework",
  "scripts": {
    "rebuild:dev-configs": "bun run --filter \"@venizia/dev-configs\" rebuild",
    "rebuild:docs": "bun run --filter \"@venizia/ignis-docs\" docs:build",
    "rebuild:mcp-docs-server": "bun run --filter \"@venizia/ignis-docs\" mcp:build",
    "rebuild:helpers": "bun run --filter \"@venizia/ignis-helpers\" rebuild",
    "rebuild:inversion": "bun run --filter \"@venizia/ignis-inversion\" rebuild",
    "prerebuild:core": "bun run rebuild:inversion && bun run rebuild:helpers",
    "rebuild:core": "bun run --filter \"@venizia/ignis\" rebuild",
    "docs:dev": "bun run --filter \"@venizia/ignis-docs\" docs:dev",
    "mcp:dev": "bun run --filter \"@venizia/ignis-docs\" mcp:dev"
  },
  "keywords": [],
  "author": {
    "name": "VENIZIA AI Developer",
    "email": "developer@venizia.ai",
    "url": "https://venizia.ai"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "bun": ">=1.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4"
  }
}
